{
  "name": "project-nitro",
  "version": "0.0.0",
  "description": "Nitro is a Node.js application for simple and complex frontend development with a tiny footprint. This app is based on generator-nitro <%= version %>",
  "license": "UNLICENSED",
  "private": true,
  "author": "The Nitro Team",
  "engines": {
    "node": ">=8 <11",
    "npm": ">=5 <7"
  },
  "scripts": {
    "nitro:pattern": "yo nitro:pattern",
    "nitro:update": "npm install generator-nitro --save-exact && yo nitro",
    "lint": "npm-run-all lint:*",
    "lint:accessibility": "gulp lint-accessibility",
    "lint:css": "stylelint src/**/*.*ss",
    "lint:data": "nitro-app-validate-pattern-data",
    "lint:html": "gulp lint-html",
    "lint:js": "eslint ./src",
    "lint:license": "license-checker --production --summary --exclude \"Apache-2.0, BSD, ISC, LGPL, MIT, MPL\" --failOn \"AGPL; EPL; GPL\"",
    "lint:ts": "tslint 'src/**/*.{ts,tsx}'",
    "postinstall": "npm rebuild node-sass",
    "prepublish": "nsp check --output summary",
    "prettier": "prettier --write \"**/*.*(js|jsx|ts|tsx|json)\"",
    "visual-approve": "gulp visual-approve",
    "visual-reference": "npm run build && gulp visual-reference",
    "visual-test": "npm run build && gulp visual-test",
    "test": "npm-run-all test:*",
    "test:lint": "npm run lint",
    "test:karma": "npm run build && karma start ./tests/karma/karma.conf.js",
    "prebuild": "npm test",
    "build": "npm-run-all build:*",
    "build:clean": "rimraf public/assets",
    "build:assets": "gulp assets",
    "build:webpack": "webpack --mode production --config config/webpack/webpack.config.prod.js",<% if (options.exporter) { %>
    "export": "npm run build && gulp export",<% } %>
    "dev": "gulp develop",
    "prod": "npm-run-all prod:*",
    "prod:build": "npm run build",
    "prod:serve": "cross-env NODE_ENV=production gulp serve",
    "start": "npm-run-all start:*",
    "start:dev": "npm run dev"
  },
  "keywords": [
    "frontend",
    "app",
    "nitro",
    "component",
    "pattern",
    "modular"
  ],
  "dependencies": {
    "@babel/polyfill": "7.0.0-rc.1"<% if (options.exampleCode) { %>,
    "bootstrap": "4.1.3",
    "handlebars": "4.0.11",
    "jquery": "3.3.1",
    "lazysizes": "4.1.0",
    "lodash": "4.17.10",
    "prevent-window-opener-attacks": "0.2.4",
    "svg4everybody": "2.1.9",
    "terrific": "3.0.0"<% } %>
  },
  "devDependencies": {
    "@namics/eslint-config": "6.0.0",
    "@namics/stylelint-bem": "5.1.1",
    "@namics/tslint-config": "0.1.0",
    "@nitro/app": "0.3.1",
    "@nitro/exporter": "0.3.1",
    "@nitro/gulp": "0.3.2",
    "@nitro/webpack": "0.3.2",
    "ajv": "6.5.3",
    "body-parser": "1.18.3",
    "browser-sync": "2.24.6",
    "chalk": "2.4.1",
    "compression": "1.7.3",
    "config": "2.0.1",
    "cross-env": "5.2.0",
    "del": "3.0.0",
    "dot-object": "1.7.0",
    "es6-promise": "4.2.4",
    "eslint": "5.4.0",
    "eslint-plugin-import": "2.14.0",
    "express": "4.16.3",
    "extend": "3.0.2",
    "generator-nitro": "<%= version %>",
    "get-port": "4.0.0",
    "globby": "7.1.1",
    "gulp": "3.9.1",
    "gulp-load-plugins": "1.5.0",
    "gulp-sequence": "1.0.0",
    "husky": "1.0.0-rc.13",
    "htmllint": "0.7.2",
    "jasmine-core": "3.2.1",
    "karma": "2.0.5",
    "karma-babel-preprocessor": "8.0.0-beta.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "license-checker": "20.2.0",
    "lint-staged": "7.2.2",
    "npm-run-all": "4.1.3",
    "nsp": "3.2.1",
    "prettier": "1.14.2",
    "rimraf": "2.6.2",
    "stylelint": "9.5.0",
    "tslint": "5.11.0",
    "typescript": "3.0.1",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.3",
    "yo": "2.0.5"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "ie 11",
    "android 4",
    "Firefox ESR"
  ],
  "postcss": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(css|scss)": [
      "stylelint"
    ],
    "src/**/*.json": [
      "prettier --list-different --write \"**/*.*(json)\"",
      "npm run lint:data"
    ],
    "src/**/*.js": [
      "prettier --list-different --write \"**/*.*(js|jsx|ts|tsx)\"",
      "eslint"
    ]
  }
}
